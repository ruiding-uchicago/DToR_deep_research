# FET RAG Production Configuration
# Use this configuration for FET sensor data research

# -- LLM Configuration --
LLM_PROVIDER=ollama
LOCAL_LLM=deepseek-r1:14b
OLLAMA_BASE_URL=http://localhost:11434/
LMSTUDIO_BASE_URL=http://localhost:1234/v1
STRIP_THINKING_TOKENS=True

# -- Research Mode Configuration --
RESEARCH_MODE=single

# -- Web Search Configuration --
SEARCH_API=duckduckgo
MAX_WEB_RESEARCH_LOOPS=2
FETCH_FULL_PAGE=True

# -- FET RAG Configuration --
# IMPORTANT: Set USE_LOCAL_RAG=True to enable FET RAG
USE_LOCAL_RAG=True

# Path to FET sensor vectorstores (comma-separated)
# The system will automatically detect and use FET RAG when it finds "excel_vectorstore" in the path
VECTOR_STORE_PATHS=/Users/ruiding/mac_python_folder/MADE_PUBLIC_REPO_snapshot2025/excel_raw_data_vectorization/excel_vectorstore,/Users/ruiding/mac_python_folder/MADE_PUBLIC_REPO_snapshot2025/non_excel_vectorstore

# Number of results to retrieve
LOCAL_RESULTS_COUNT=10

# Embedding model (must match the one used for vectorization)
EMBEDDING_MODEL=BAAI/bge-m3

# -- Tree of Thoughts (DToR) Configuration --
MAX_BRANCHES=3
MAX_BRANCH_DEPTH=2
NODES_PER_BRANCH=100